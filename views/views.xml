<odoo>
  <data>
    <!--odooform-->
    <record id="view_thuani_dashboard_form" model="ir.ui.view">
        <field name="name">view.thuani.dashboard.form</field>
        <field name="model">thuani.dashboard</field>
        <field name="arch" type="xml">
            <form string="thuani Dashboard">
                <sheet>
                  <!--odooformtitle-->
                  <div class="oe_title">
                      <label for="name" class="oe_edit_only"/>
                      <h1>
                          <field name="name" placeholder="Blog URL"/>
                      </h1>
                  </div>
                    <group>
                        <field name="thuani_blog_url_permalink"/>
                        <field name="thuani_blog_url_category"/>
                        <field name="thuani_blog_url_postname"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>
    <!--odootree-->
    <record id="view_thuani_dashboard_tree" model="ir.ui.view">
        <field name="name">view.thuani.dashboard.tree</field>
        <field name="model">thuani.dashboard</field>
        <field name="arch" type="xml">
            <tree string="thuani Dashboard Tree">
                <field name="thuani_blog_url_permalink"/>
                <field name="thuani_blog_url_category"/>
                <field name="thuani_blog_url_postname"/>
            </tree>
        </field>
    </record>

    <!--odooviewaction-->
    <record id="action_thuani_dashboard_views" model="ir.actions.act_window">
        <field name="name">thuani Dashboard</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">thuani.dashboard</field>
        <field name="view_mode">tree,form</field>
        <field name="help" type="html">
          <p class="oe_view_nocontent_create">
            thuani Dashboard
          </p><p>
            Click to start your thuani Business Website
          </p>
        </field>
    </record>
    <!--odoomenuitemroot-->
    <!-- This Menu Item will appear in the Upper bar, That's why It needs NO parent or action -->
    <menuitem id="menu_thuani_dashboard_root" name="thuani" sequence="1"/>
    <!--odoomenuitemcateg-->
    <!-- This Menu Item Must have a parent -->
    <menuitem id="menu_thuani_dashboard_categ" name="Dashboard" parent="menu_thuani_dashboard_root" sequence="1"/>
    <!--odoomenuitemaction-->
    <!-- This Menu Item must have a parent and an action -->
    <menuitem id="menu_thuani_dashboard" name="thuani Dashboard" parent="menu_thuani_dashboard_categ" action="action_thuani_dashboard_views" sequence=""/>

  </data>
</odoo>